<thead>
  <tr>
    <th><%= User.human_attribute_name(:name)%> | </th>
    <th><%= User.human_attribute_name(:email)%></th>
  </tr>
</thead>
<br>
<tbody>
  <% @users.each do |user| %>
     <%= content_tag :tr, id: dom_id(user), class: dom_class(user) do %>
        <td><%= user.name %> | </td>
        <td><%= user.email %></td>
      <% end %>
    <% end %>
  </tbody>
